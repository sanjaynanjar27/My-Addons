<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_partner_inherit_form_restriction" model="ir.ui.view">
        <field name="name">Partner Email Restriction Field</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="restrict_mail" string="Restrict Mail"/>
                </xpath>
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="credit_limit" string="Credit Limit"/>
                </xpath>
            </data>
        </field>
    </record>
</odoo>

