<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_project_start_end_date_wizard_form" model="ir.ui.view">
        <field name="name">project.custom.wizard</field>
        <field name="model">select.date.range.wizard</field>
        <field name="arch" type="xml">
            <form string="Select Date Range">
                <sheet>
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Print Report" name="print_date_range_report" class="btn btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>