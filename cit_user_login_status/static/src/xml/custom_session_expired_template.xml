<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="cit_user_login_status.CustomSessionExpiredMSG"
     t-inherit="web.SessionExpiredDialog"
     t-inherit-mode="extension">
     <xpath expr="//p[hasclass('text-prewrap')]"    position="replace">
         <p class="text-prewrap">
            Your Odoo session has timed out. Please refresh the page to continue.
         </p>
     </xpath>
    </t>
</templates>